= form_for Image.new do |f|
  -@images.each do |link|
    .stolen_pics
      .images_block
        .image
          = image_tag link

        = f.check_box_tag(:select)

        %select{:name=>'category'}
          -@categories.each do |cat|
            %option{:value=>"#{cat.id}"}=cat.title

  %b= paginate @images